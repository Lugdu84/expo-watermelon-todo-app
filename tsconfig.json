{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"strict": true,
		"baseUrl": "./",
		"paths": {
			"@/*": ["./src/*"],
			"@components/*": ["./src/components/*"],
			"@database/*": ["./src/database/*"],
			"@lib/*": ["./src/lib/*"]
		}
	},
	"include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts"]
}
